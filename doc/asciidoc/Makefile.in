dist-mans: $(man1_MANS:%=$(MAN_DOCSDIR)%)
	docs="$^"; dest=$(GEN_DOC_DIST_ROOT)/man; \
	$(mkinstalldirs) $$dest && \
	for f in $$docs; do \
	    $(INSTALL_DATA) $$f $$dest; \
